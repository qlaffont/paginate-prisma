{
  "version": "1.0.100",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "eslint src test --fix",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "np": "np",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "paginate-prisma",
  "author": "flexper",
  "repository": {
    "type": "git",
    "url": "https://github.com/flexper/paginate-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/flexper/paginate-prisma/issues",
    "email": "contact@flexper.fr"
  },
  "module": "dist/paginate-prisma.esm.js",
  "size-limit": [
    {
      "path": "dist/paginate-prisma.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/paginate-prisma.esm.js",
      "limit": "10 KB"
    }
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ]
  },
  "pnpm": {
    "overrides": {
      "typescript": "4.9.5"
    }
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@prisma/client": "^4.9.0",
    "@size-limit/preset-small-lib": "8.2.6",
    "@types/node": "18.16.19",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "graphql": "^16.6.0",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "prisma": "^4.9.0",
    "reflect-metadata": "^0.1.13",
    "size-limit": "8.2.6",
    "tsdx": "0.14.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6",
    "wds": "0.17.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "class-validator": "^0.14.0",
    "lodash": "^4.17.21"
  }
}
